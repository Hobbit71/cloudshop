# CloudShop Customer Service Environment Variables

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cloudshop
DB_USER=cloudshop
DB_PASSWORD=cloudshop

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Elasticsearch Configuration
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200

# Service Configuration
PORT=3004
SPRING_PROFILES_ACTIVE=dev

# CORS Configuration
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Logging
LOG_LEVEL=DEBUG
SQL_LOG_LEVEL=WARN
SQL_PARAM_LOG_LEVEL=WARN

# Hibernate DDL
DDL_AUTO=validate
SHOW_SQL=false

